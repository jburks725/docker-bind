$TTL 604800
@ IN  SOA dns.jasonburks.local. jason.jasonburks.com. (
            7   ; Serial
       604800   ; Refresh
        86400   ; Retry
      2419200   ; Expire
       604800 ) ; Negative Cache TTL
;
$TTL  86400
@     IN    NS    dns.jasonburks.local.
1.1   IN    PTR   router.jasonburks.local.
1.4   IN    PTR   esx.jasonburks.local.
1.5   IN    PTR   docker.jasonburks.local.
